{
  "name": "DiscordMusicBot",
  "description": "A simple discord music bot with the discord.js.",
  "repository": "https://github.com/bear3z/AurasDiscordBot",
  "logo": "https://cdn.discordapp.com/avatars/750613142488481843/e6326038dbe2243ca551ba5b6ecd8bf2.png?size=1024",
  "keywords": ["node", "discord", "youtube","music","bot"],
  "image": "heroku/nodejs",
  "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "TOKEN": {
      "description": "The token copied in the first section.",
      "required": "true"
    },
    "PREFIX": {
      "description": "The prfix you want to use with the bot.",
      "value": "!"
    },
    "YTAPI": {
      "description": "Your YouTube API key.",
      "required": "true"
    }
  }  
}
